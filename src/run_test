# make distclean; make; clear
rm -rf data catalog
for i in $(eval echo {1..$1});
do
	echo "Running test $i..."
	./server & sleep 0.20; cat ../project_tests/test$(printf "%02d" $i).dsl | ./client | diff ../project_tests/test$(printf "%02d" $i).exp -
	echo "Done with test $i!"
	echo "==================="
done 
